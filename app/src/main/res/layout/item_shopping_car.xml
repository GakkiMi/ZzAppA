<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/shape_gouwuche_item"
    android:padding="15dp">

    <ImageView
        android:id="@+id/warehouse_iv_check_status"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:src="@drawable/icon_no_select" />

    <TextView
        android:id="@+id/warehouse_tv_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_toRightOf="@+id/warehouse_iv_check_status"
        android:drawableStart="@mipmap/icon_cangku"
        android:drawablePadding="@dimen/dp_5"
        android:text="仓库1"
        android:textColor="#ff333333"
        android:textSize="@dimen/sp_13" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycleViewInner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/warehouse_iv_check_status" />
</RelativeLayout>

    <!--</androidx.cardview.widget.CardView>-->

